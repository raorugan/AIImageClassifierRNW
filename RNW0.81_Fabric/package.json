{
  "name": "AIImageClassifierRNW",
  "version": "1.0.0",
  "private": true,
  "description": "AI Image Classifier for Windows using React Native Windows 0.79 (Paper Architecture - ARM64)",
  "scripts": {
    "windows": "npx @react-native-community/cli run-windows",
    "windows-release": "react-native run-windows --arch ARM64 --release",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:windows": "jest --config jest.config.windows.js"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.81.5",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-converter": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-windows": "0.81.0",
    "seedrandom": "^3.0.5",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@rnx-kit/jest-preset": "^0.1.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "metro-config": "^0.82.5",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "AIImageClassifierRNW",
      "namespace": "AIImageClassifierRNW",
      "template": "cpp-app"
    }
  }
}
